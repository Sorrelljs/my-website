{
    "extends": "eslint:recommended",
    "rules": {
        "no-alert": 1,
        "camelcase": 1,
        "curly": 1,
        "eqeqeq": 1,
        "no-console": 1,
        "guard-for-in": 1,
        "no-empty": 1,
        "no-param-reassign": 1,
        "no-unused-vars": 1,
        "quotes": [ 1, "single"],
        "no-extra-parens": 1,
        "no-extra-semi": 1
    },
    "globals":{ 
        "$": true,
        "jQuery": true
    },
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    }
}
